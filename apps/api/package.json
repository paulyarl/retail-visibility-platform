{
  "name": "@rvp/api",
  "version": "1.0.0",
  "description": "RVP API server (Express + TS + Prisma + SQLite) - Force rebuild v2",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:local": "doppler run --config local -- nodemon",
    "build": "node build-with-selective-errors.js",
    "start": "node dist/index.js",
    "vercel-build": "node build-with-selective-errors.js",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:local": "tsx prisma/seed.ts",
    "db:push": "doppler run --config dev -- prisma db push",
    "db:push:local": "doppler run --config local -- prisma db push",
    "db:push:prod": "doppler run --config prd -- prisma db push",
    "db:migrate": "doppler run --config dev -- prisma migrate dev",
    "db:migrate:local": "doppler run --config local -- prisma migrate dev",
    "db:studio": "doppler run --config dev -- prisma studio",
    "db:studio:local": "doppler run --config local -- prisma studio",
    "db:generate": "doppler run --config dev -- prisma generate",
    "dev:migrate:deploy": "doppler run --config dev -- prisma migrate deploy",
    "dev:generate": "doppler run --config dev -- prisma generate",
    "dev:seed": "doppler run --config dev -- tsx prisma/seed.ts",
    "dev:create-admin": "doppler run --config dev -- node create-admin-user.js",
    "dev:create-tenants": "doppler run --config dev -- node create-test-tenants.js --type=retail --count=3",
    "dev:create-chain": "doppler run --config dev -- node create-test-chain-enhanced.js --scenario=retail",
    "bootstrap:dev": "npm run dev:migrate:deploy && npm run dev:generate && npm run dev:seed && npm run dev:create-admin && npm run dev:create-tenants && npm run dev:create-chain",
    "local:migrate:deploy": "doppler run --config local -- prisma migrate deploy",
    "local:generate": "doppler run --config local -- prisma generate",
    "local:seed": "doppler run --config local -- tsx prisma/seed.ts",
    "local:create-admin": "doppler run --config local -- node create-admin-user.js",
    "local:create-tenants": "doppler run --config local -- node create-test-tenants.js --type=retail --count=3",
    "local:create-chain": "doppler run --config local -- node create-test-chain-enhanced.js --scenario=retail",
    "bootstrap:local": "npm run local:migrate:deploy && npm run local:generate && npm run local:seed && npm run local:create-admin && npm run local:create-tenants && npm run local:create-chain",
    "migrate:data": "node --experimental-json-modules ./scripts/migrate.js",
    "migrate:storage": "node --experimental-json-modules ./scripts/storage-sync.js",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:singletons": "node test-singletons.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "schema:validate": "node scripts/validate-prisma-schema.js",
    "migration:check": "node scripts/pre-migration-check.js",
    "migration:dev": "node scripts/migration-wrapper.js dev",
    "migration:deploy": "node scripts/migration-wrapper.js deploy",
    "migration:reset": "node scripts/migration-wrapper.js reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@google/genai": "^1.0.0",
    "@google/generative-ai": "^0.24.1",
    "@paypal/paypal-server-sdk": "^2.1.0",
    "@prisma/client": "^6.17.1",
    "@sendgrid/mail": "^8.1.4",
    "@sentry/node": "^10.32.1",
    "@supabase/supabase-js": "^2.90.1",
    "@types/speakeasy": "^2.0.10",
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "execa": "^9.6.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^165.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "2.0.2",
    "nanoid": "^5.1.6",
    "newrelic": "^13.8.1",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "speakeasy": "^2.0.0",
    "square": "^43.2.0",
    "stripe": "^17.5.0",
    "tslib": "^2.6.2",
    "ua-parser-js": "^2.0.7",
    "validator": "^13.15.26",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.15.10",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.19.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "zx": "^8.8.5"
  }
}
