  tenant @map("tenant")
  policy @map("policy")
  oauth @map("oauth")
  other @map("other")
  @@map("entity_type")
}

enum ItemStatus {
  active @map("active")
  inactive @map("inactive")
  archived @map("archived")
  trashed @map("trashed")
  @@map("item_status")
}

enum ItemVisibility {
  public @map("public")
  private @map("private")
  @@map("item_visibility")
}

enum JobStatus {
  queued @map("queued")
