{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/index.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run vercel-build"
}
